import math

class Complex:
    def __init__(self, real, imaginary):
        self.real = real
        self.imaginary = imaginary

    def __add__(self, no):
        return Complex(self.real + no.real, self.imaginary + no.imaginary)
    
    def __sub__(self, no):
        return Complex(self.real - no.real, self.imaginary - no.imaginary)
    
    def __mul__(self, no):
        r=self.real * no.real - self.imaginary * no.imaginary
        i=self.real * no.imaginary + no.real * self.imaginary
        return Complex(r,i)
    
    def __truediv__(self, no):
        Denominator=no.real**2 + no.imaginary**2
        numerator_r=self.real * no.real + self.imaginary * no.imaginary
        numerator_i=self.imaginary * no.real - no.imaginary * self.real
        return Complex(numerator_r / Denominator, numerator_i / Denominator)
    
    def mod(self):
        return Complex(math.sqrt(self.real**2 + self.imaginary**2),0)

    def __str__(self):
        if self.imaginary == 0:
            result = "%.2f+0.00i" % (self.real)
        elif self.real == 0:
            if self.imaginary >= 0:
                result = "0.00+%.2fi" % (self.imaginary)
            else:
                result = "0.00-%.2fi" % (abs(self.imaginary))
        elif self.imaginary > 0:
            result = "%.2f+%.2fi" % (self.real, self.imaginary)
        else:
            result = "%.2f-%.2fi" % (self.real, abs(self.imaginary))
        return result


c = map(float, input().split())
d = map(float, input().split())
x = Complex(*c)
y = Complex(*d)

print(x+y)
print(x-y)
print(x*y)
print(x/y)
print(x.mod())
print(y.mod())
